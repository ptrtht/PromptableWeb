{
  "name": "sveltefast",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run supa:types && vite dev --port 5175 ",
    "build": "vite build",
    "preview": "svelte-kit sync && vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "supa:types": "npx supabase gen types typescript --project-id dkuazlvtqqhwavflsjjb --schema public > src/lib/services/schemas/supabase.types.ts && npx supazod -i src/lib/services/schemas/supabase.types.ts -o src/lib/services/schemas/supabase.schemas.ts -t src/lib/services/schemas/supabase.schemas.d.ts -s public",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-svelte-csf": "^5.0.0-next.21",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/svelte": "^8.4.7",
    "@storybook/sveltekit": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-vercel": "^5.5.2",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.0.0-next.77",
    "clsx": "^2.1.1",
    "daisyui": "^4.12.14",
    "lucide-svelte": "^0.469.0",
    "mode-watcher": "^0.5.0",
    "postcss": "^8.4.49",
    "storybook": "^8.4.7",
    "supazod": "^1.1.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-sonner": "^0.3.28",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "highlight.js": "^11.11.1",
    "loadash": "^1.0.0",
    "monaco-editor": "^0.52.2",
    "openai": "^4.77.0",
    "supabase": "^2.0.0",
    "zod": "^3.24.1"
  }
